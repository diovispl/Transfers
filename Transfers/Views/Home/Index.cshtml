<div class="accordion" id="panelSearch" style="display: none;">
    <div class="accordion-group">
        <div class="accordion-heading">
            <a class="accordion-toggle" data-toggle="collapse" data-parent="#panelSearch" href="#collapseOne">Search Data
            </a>
        </div>
        <div id="collapseOne" class="accordion-body collapse in">
            <div id="ElementSearch" class="accordion-inner">
            </div>
        </div>
    </div>
</div>


<div class="jumbotron" id="PanelSearch">


    <form class="form-search">
        <div id="SearchFrom" style="padding-top: 20px;">
            <h4 class="text-info">Pick up point from:</h4>
            <input type="text" id="MT_SearchFrom" placeholder="Search" class="glyphicon glyphicon-search" style="width: 410px;">
        </div>
        <div id="dFrom" style="display: none;">
            <h4 class="text-info">Transfer Date</h4>
            <div class="navbar-inner" style="display: inline-block;">
                <div id="dateFrom">
                </div>
            </div>
        </div>
        <div id="tFrom" style="display: none;">
            <h4 class="text-info">Transfer Time</h4>
            <div class="navbar-inner" style="display: inline-block;">
                <select multiple="multiple" id="timeFrom" style="width: 80px; margin-top: 15px; margin-bottom: 15px; height: 200px;">
                    @for (int i = 0; i <= 23; i++)
                    {
                        <option>@Html.Raw(i.ToString("00") + ":00")</option>
                        <option>@Html.Raw(i.ToString("00") + ":15")</option>
                        <option>@Html.Raw(i.ToString("00") + ":30")</option>
                        <option>@Html.Raw(i.ToString("00") + ":45")</option>
                    }
                </select>
            </div>
        </div>


        <div id="alertReturn" class="alert alert-info" style="margin-top: 10px; display: none;">
            <h4>Do you want a return day?</h4>
            <a class="btn btn-info btn-large" onclick="GoToDestination();">Yes
            </a>
            <a class="btn btn-large" onclick="Search();">No
            </a>
        </div>

        <div id="SearchTo" style="padding-top: 20px; display: none;">
            <h4 class="text-info">Destination to:</h4>
            <input type="text" id="MT_SearchTo" placeholder="Search" class="glyphicon glyphicon-search" style="width: 410px;">
        </div>

        <div id="dTo" style="display: none;">
            <h4 class="text-info">Return Date</h4>
            <div class="navbar-inner" style="display: inline-block;">
                <div id="dateTo">
                </div>
            </div>
        </div>
        <div id="tTo" style="display: none;">
            <h4 class="text-info">Return Time</h4>
            <div class="navbar-inner" style="display: inline-block;">
                <select multiple="multiple" id="timeTo" style="width: 80px; margin-top: 15px; margin-bottom: 15px; height: 200px;">
                    @for (int i = 0; i <= 23; i++)
                    {
                        <option>@Html.Raw(i.ToString("00") + ":00")</option>
                        <option>@Html.Raw(i.ToString("00") + ":15")</option>
                        <option>@Html.Raw(i.ToString("00") + ":30")</option>
                        <option>@Html.Raw(i.ToString("00") + ":45")</option>
                    }
                </select>
            </div>
        </div>



    </form>


    <br />


</div>

